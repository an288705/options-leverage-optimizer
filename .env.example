# Interactive Brokers API Configuration
# Copy this file to .env and fill in your actual credentials

# Interactive Brokers Client Portal API Base URL
VITE_IB_API_BASE_URL=https://localhost:5001/v1/api

# Interactive Brokers Account ID
VITE_IB_ACCOUNT_ID=your_account_id_here

# Enable/Disable Mock Mode (set to 'false' to use real API)
VITE_USE_MOCK_DATA=true

# API Request Timeout (milliseconds)
VITE_API_TIMEOUT=10000
